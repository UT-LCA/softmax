 .timescale 0 0;
v0x16d2b80 .array "memory", 7 0, 15 0;
v0x16d2c40_0 .var "n", 4 0;
    .scope S_0x16d29d0;
T_0 ;
    %vpi_call 2 6 "$readmemh", "mem1.txt", v0x16d2b80 {0 0 0};
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x16d2c40_0, 0, 5;
T_0.0 ;
    %load/vec4 v0x16d2c40_0;
    %pad/u 32;
    %cmpi/u 7, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %vpi_call 2 8 "$display", "%h", &A<v0x16d2b80, v0x16d2c40_0 > {0 0 0};
    %load/vec4 v0x16d2c40_0;
    %addi 1, 0, 5;
    %store/vec4 v0x16d2c40_0, 0, 5;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
